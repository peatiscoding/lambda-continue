{
  "verbose": true,
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testEnvironment": "node",
  "testRegex": "(/__jest__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "testPathIgnorePatterns": [
    "/node_modules"
  ]
}